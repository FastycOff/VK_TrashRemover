{
    "manifest_version": 2,
    "name": "VkTrashRemover",
    "description": "Removing trash.",
    "version": "0.0.1",
    "author": "Fastyc",
    "browser_action": {
        "default_title": "Vk trash remover"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://vk.com/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
